    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@preact/preset-vite": "^2.9.0",
    "@types/node": "^22.0.0",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "eslint": "^8.57.0",
    "eslint-config-next": "^15.1.0",
    "jsdom": "^25.0.0",
